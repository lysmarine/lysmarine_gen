[auto-show]
enabled=true

[theme-settings]
key-size=95.0
key-gradient-direction=-5.0
color-scheme='/usr/share/onboard/themes/Charcoal.colors'
key-fill-gradient=4.0
key-style='gradient'
key-stroke-gradient=15.0
key-label-font='Sans'
key-shadow-strength=70.0
key-shadow-size=30.0
roundrect-radius=20.0

[/]
current-settings-page=7
use-system-defaults=false
theme='/usr/share/onboard/themes/Droid.theme'
system-theme-associations={'Default': '/usr/share/onboard/themes/Droid.theme'}
layout='/usr/share/onboard/layouts/Compact.onboard'
schema-version='2.3'
snippets=@as []
show-status-icon=true
status-icon-provider="GtkStatusIcon"

[window]
docking-enabled=true

[window/landscape]
width=764
height=480
x=36
y=0
